{
  "expo": {
    "name": "pomp",
    "slug": "pomp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pompei-icon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adedigba.pomp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-pompei-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adedigba.pomp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pompei-icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "edd657a0-ef34-4d0c-8688-ca695f5ab651"
      }
    }
  }
}
